#include<stdio.h>//比较大小
get_max(int x,int y){
  if(x>y){
  return x;
  }
  else{
  return y;
  }
  
}

int main()
{
  int a = 20;
  int b = 30;
  int max = get_max(a,b);
  printf("%d",max);
  return 0;
}
#include<stdio.h>
void Swap(int *pa,int *pb)
{
  int temp = 0;
  temp =*pa;
  *pa = *pb;
  *pb = temp;
}
   
int main()
{
  int a =20;
  int b =30;
  print("a=%d b=%d\n",a,b);
  Swap(&a,&b);
  printf("a=%d b=%d\n",a,b);
  return 0;
}
