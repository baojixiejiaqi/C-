#include<stdio.h>
int Add(int a, int b) {
	return a + b;
}
int Sub(int a, int b) {
	return a - b;
}
int Mul(int a, int b) {
	return a * b;
}
int Div(int a, int b) {
	return a / b;
}
int main()
{
	int* arr[5];//指针数组
	int(*p[5])(int, int) = { Add,Sub,Mul,Div };//需要一个数组，这个数组可以存放四个函数的地址-函数指针的数组
	int i = 0;
	for (i = 0; i < 5; i++) {
		printf("%d\n", p[i](10, 2));
	}

}
