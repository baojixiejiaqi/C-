#include<stdio.h>//试除法
int main() {		
	int i = 0;
	int count = 0;
	for (i = 100; i <= 200; i++) {
		for (j=2; j <i; j++) {
			if (i % j == 0) {
				break;
			}
		}
		if (j == i) {
			printf("%d ", i);
			count++;
		}
	}
	printf("\ncount=%d ", count);
	return 0;

}
#include<stdio.h>//优化后
#include<math.h>
int main() {		
	int i = 0;
	int count = 0;
	for (i = 101; i <= 200; i+=2) {
		int j = 0;
		for (j=2; j <sqrt(i); j++) {
			if (i % j == 0) {
				break;
			}
		}
		if (j >sqrt(i)) {
			printf("%d ", i);
			count++;
		}
	}
	printf("\ncount=%d ", count);
	return 0;

}
