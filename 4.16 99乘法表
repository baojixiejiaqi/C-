#include<stdio.h>
int main()
{
	int i = 0;
	for (i = 1; i <= 9; i++) {
		int j = 1;
		for(j = 1; j <= i; j++){
			printf("%d*%d=%-2d ",i,j,i * j);
		}
		printf("\n");
	}
	return 0;
}
#include <stdio.h>
times(int n)
{
	int i = 0;
	for (i = 1; i <= n; i++) {
		int j = 0;
		for (j = 1; j <=i; j++) {
			printf("%d * %d = %-2d ", i, j, i * j);
		}
		printf("\n");
	}
	
}
int main()
{
	int n = 0;
	scanf("%d", &n);
	times(n);
	return 0;
}
