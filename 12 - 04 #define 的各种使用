#define PRINT(val ,format)	printf("The value of "#val " is "format"\n",val)
int main()
{
	int a = 5;
	PRINT(a, "%d");
	int b = 8;
	PRINT(b, "%d");
	double c = 10.0;
	PRINT(c, "%lf");
}
