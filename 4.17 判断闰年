#include<stdio.h>
int main() {		
	int year = 0;
	int count = 0;
	for (year=1000; year <= 2000; year++) {
		if (year % 4 == 0 && year % 100 != 0||year %400==0) {
			printf("%d ", year);
			count++;
		}
	}
	printf("\ncount: %d ", count);
	return 0;


}
#include<stdio.h>
int is_leap_year(int i)
{
	if ((i % 4== 0 && i % 100!= 0) || (i % 400 == 0)) {
		return 1;
	}
	else 
	{
		return 0;
	}
}
int main()
{
	int year = 0;
	for (year = 1000; year <= 2000; year++)
	{
		if (is_leap_year(year) == 1)
		{
			printf("%d ", year);
		}
	}return 0;
}
