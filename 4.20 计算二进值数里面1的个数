#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	int num = 0;
	int count=0;
	scanf("%d", &num);
	while (num)
	{
		if (num % 2 == 1) {
			count++;
			num = num / 2;
		}
	}
	printf("%d", count);
	return 0;

}
#include<stdio.h>
int main()
{
	int num = 0;
	int count = 0;
	scanf("%d", &num);
	int i = 0;
	for (i = 0; i < 32; i++) {
		if (((num >> i) & 1) == 1) {
			count++;
		}
	}
	printf("%d", count);
	return 0;
}
#include <stdio.h>
int main()
{
 int num = -1;
 int i = 0;
 int count = 0;//计数
 while(num)
 {
 count++;
 num = num&(num-1);
 }
 printf("二进制中1的个数 = %d\n",count);
 return 0;
}
#include <stdio.h>//利用函数计算
int count_one(int i) {
	int count = 0;
	while(i) {
		if (i % 2 == 1) {
			count++;
		}
		i =i/ 2; 
	}
	return count;
}
int main(){
	int i = 0;
	scanf("%d", &i);
	int count = count_one(i);
	printf("%d 里面含有%d个1",i,count);
	return 0;
}
