#define _CRT_SECURE_NO_WARNINGS 1 //6分代码
#include<stdio.h>
void my_strcpy(char* dest, char* src) //6分代码
{
	while (*src != '\0') 
	{
		*dest = *src;
		src++;
		dest++;
	}
	*dest = *src;
}
int main()
{
	char arr1[] = "*************";
	char arr2[] = "bit";
	my_strcpy(arr1, arr2);
	printf("%s\n", arr1);
	return 0;
}
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
void my_strcpy(char* dest, char* src)//8分代码
{
	while (*src != '\0')
	{
		*dest++ = *src++;//先执行解引用，再执行后置++操作
	
	}
	*dest = *src;
}
int main()
{
	char arr1[] = "*************";
	char arr2[] = "bit";
	my_strcpy(arr1, arr2);
	printf("%s\n", arr1);
	return 0;

}
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
void my_strcpy(char* dest, char* src)//10分
{
	while (*dest++ = *src++)//借助\0的阿斯克码值为0，进入循环，条件为假所以直接跳出循环结束
	{
		;

	}
	
}
int main()
{
	char arr1[] = "*************";
	char arr2[] = "bit";
	my_strcpy(arr1, arr2);
	printf("%s\n", arr1);
	return 0;

}
